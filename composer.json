{
    "name": "jwage/phpchunkit",
    "type": "library",
    "description": "PHPUnit Test Runner",
    "keywords": ["tests", "phpunit"],
    "homepage": "http://github.com/jwage/phpchunkit",
    "license": "MIT",
    "authors": [
        {"name": "Jonathan H. Wage", "email": "jonwage@gmail.com"},
        {"name": "Kris Wallsmith", "email": "kris.wallsmith@gmail.com"}
    ],
    "autoload": {
        "psr-4": {
            "PHPChunkit\\": "src/",
            "PHPChunkit\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "phpunit/phpunit": "5.5.*",
        "symfony/console": "^3.1",
        "symfony/process": "^3.1",
        "symfony/stopwatch": "^3.1",
        "symfony/finder": "^3.1",
        "symfony/event-dispatcher": "^3.1"
    },
    "require-dev": {
        "ext-pdo": "*",
        "ext-pdo_mysql": "*"
    },
    "bin": [
        "bin/phpchunkit"
    ]
}
